<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidcashier">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH_CONNECT" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/unoe"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidCashier"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ui.sellsByCategory.SellsByCategory"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ui.invoice.ReternActivity"
            android:exported="false" />
        <activity
            android:name=".ui.ReportsItemsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.reportActivity"
            android:exported="false" />
        <activity
            android:name=".ui.ReportsActivity"
            android:exported="true" />
        <activity
            android:name=".ui.closed.ClosedActivity"
            android:exported="true" />
        <activity
            android:name=".ui.TestPaginationActivity"
            android:exported="true" />
        <activity
            android:name=".ui.bayWay.ChoisePayActivity"
            android:exported="true" />
        <activity
            android:name=".ui.crudUI.AddItemActivity"
            android:exported="true" />
        <activity
            android:name=".ui.auth.DeleteActivity"
            android:exported="true" />
        <activity
            android:name=".ui.crudUI.UpdateProductActivity"
            android:exported="true" />
        <activity
            android:name=".ui.SelectionActivity"
            android:exported="true" />
        <activity
            android:name=".ui.auth.CreateAccount"
            android:exported="true" />
        <activity
            android:name=".ui.auth.Login"
            android:exported="true" />
        <activity
            android:name=".ui.invoice.ItemsActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.scanner.capture"
            android:screenOrientation="portrait"
            android:stateNotNeeded="true"
            android:theme="@style/zxing_CaptureTheme" />
    </application>

</manifest>